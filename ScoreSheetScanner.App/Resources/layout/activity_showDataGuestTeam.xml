<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp">
    <Button
        android:text="@string/next_Overview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/btnToOverview"
        android:layout_gravity="center_vertical"
        android:backgroundTint="@color/accent_material_light"
        android:textColor="@android:color/white"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/btnToOverview">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <!-- Player 1 D1 -->
                <TextView
                    android:id="@+id/label_Guestplayer1D1"
                    android:text="@string/guestTeamPlayer1D1"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer1D1"
                    android:layout_below="@id/label_Guestplayer1D1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                 <!-- Player 2 D1 -->
                <TextView
                    android:id="@+id/label_Guestplayer2D1"
                    android:text="@string/guestTeamPlayer2D1"
                    android:layout_below="@id/textEdit_Guestplayer1D1"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer2D1"
                    android:layout_below="@id/label_Guestplayer2D1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player 1 D2 -->
                <TextView
                    android:id="@+id/label_Guestplayer1D2"
                    android:text="@string/guestTeamPlayer1D2"
                    android:layout_below="@id/textEdit_Guestplayer2D1"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer1D2"
                    android:layout_below="@id/label_Guestplayer1D2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                 <!-- Player 2 D2 -->
                <TextView
                    android:id="@+id/label_Guestplayer2D2"
                    android:text="@string/guestTeamPlayer2D2"
                    android:layout_below="@id/textEdit_Guestplayer1D2"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer2D2"
                    android:layout_below="@id/label_Guestplayer2D2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player 1 D3 -->
                <TextView
                    android:id="@+id/label_Guestplayer1D3"
                    android:text="@string/guestTeamPlayer1D3"
                    android:layout_below="@id/textEdit_Guestplayer2D2"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer1D3"
                    android:layout_below="@id/label_Guestplayer1D3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player 2 D3 -->
                <TextView
                    android:id="@+id/label_Guestplayer2D3"
                    android:text="@string/guestTeamPlayer2D3"
                    android:layout_below="@id/textEdit_Guestplayer1D3"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_Guestplayer2D3"
                    android:layout_below="@id/label_Guestplayer2D3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E1 -->
                <TextView
                    android:id="@+id/label_GuestplayerE1"
                    android:text="@string/guestTeamPlayerE1"
                    android:layout_below="@id/textEdit_Guestplayer2D3"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE1"
                    android:layout_below="@id/label_GuestplayerE1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E2 -->
                <TextView
                    android:id="@+id/label_GuestplayerE2"
                    android:text="@string/guestTeamPlayerE2"
                    android:layout_below="@id/textEdit_GuestplayerE1"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE2"
                    android:layout_below="@id/label_GuestplayerE2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E3 -->
                <TextView
                    android:id="@+id/label_GuestplayerE3"
                    android:text="@string/guestTeamPlayerE3"
                    android:layout_below="@id/textEdit_GuestplayerE2"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE3"
                    android:layout_below="@id/label_GuestplayerE3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E4 -->
                <TextView
                    android:id="@+id/label_GuestplayerE4"
                    android:text="@string/guestTeamPlayerE4"
                    android:layout_below="@id/textEdit_GuestplayerE3"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE4"
                    android:layout_below="@id/label_GuestplayerE4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E5 -->
                <TextView
                    android:id="@+id/label_GuestplayerE5"
                    android:text="@string/guestTeamPlayerE5"
                    android:layout_below="@id/textEdit_GuestplayerE4"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE5"
                    android:layout_below="@id/label_GuestplayerE5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <!-- Player E6 -->
                <TextView
                    android:id="@+id/label_GuestplayerE6"
                    android:text="@string/guestTeamPlayerE6"
                    android:layout_below="@id/textEdit_GuestplayerE5"
                    android:paddingTop="45px"
                    android:textSize="38px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
                <android.support.design.widget.TextInputEditText
                    android:id="@+id/textEdit_GuestplayerE6"
                    android:layout_below="@id/label_GuestplayerE6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    />
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>
</RelativeLayout>
